{
  "name": "blueprint-select-testing-library",
  "version": "0.0.1",
  "description": "A library for testing UIs which use Blueprint JS's Select component.",
  "main": "lib/index.js",
  "repository": "https://github.com/codemix/component-testing-library",
  "author": "Charles Pick <charles@codemix.com>",
  "license": "MIT",
  "dependencies": {
    "component-testing-library": "^0.0.1",
    "blueprint-testing-library": "^0.0.1"
  },
  "devDependencies": {
    "@blueprintjs/core": "^3.13.0",
    "@blueprintjs/select": "^3.6.1",
    "@types/jest": "^23.3.13",
    "@types/react": "^16.8.4",
    "@types/react-dom": "^16.8.2",
    "jest": "^23.6.0",
    "prettier": "^1.16.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-testing-library": "^5.8.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.4"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "peerDependencies": {
    "@blueprintjs/core": "^3.13.0"
  }
}
